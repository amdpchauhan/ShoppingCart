          <input type="text" id="ProductImageAltText" name="ProductImageAltText" />

          <p class="contact"><label for="ProductShortDescription">Product Short Description</label></p> 
          <textarea rows="2" cols="55" name="ProductShortDescription" id="ProductShortDescription"></textarea>

          <p class="contact"><label for="ProductDetailedDescription">Product Detailed Description</label></p> 
          <textarea rows="4" cols="55" name="ProductDetailedDescription" id="ProductDetailedDescription"></textarea>

            <input class="buttom" name="submit" id="submit" tabindex="5" value="Add Product" type="submit" onclick="return validateform();">   
