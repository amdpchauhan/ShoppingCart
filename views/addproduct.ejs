  <!-- Top -->
<div class="tabbed">
      
  <div class="tab-content" style="display: block; padding-bottom:50px; padding-top:60px">
      <header style="padding-bottom:20px">
          <center><h2 style="font-size: 28px; font-weight:bold; font-style: italic;
          color: black; text-shadow: 0px 1px 1px rgba(255,255,255,0.8);">Please enter new product details::</h2></center>
      </header>  
      <div  class="form">
        <form enctype="multipart/form-data" method="post" action="/addproductaction" id="contactform" > 
          <p class="contact"><label for="ProductName">Product Name</label></p> 
          <input id="ProductName" name="ProductName" id="ProductName" placeholder="" tabindex="1" type="text"> 
          <p class="contact"><label for="ProductCategory">Product Category</label>&nbsp;&nbsp;&nbsp;&nbsp;   <select id="ProductCategory" name="ProductCategory" >
            <option value="">-SELECT-</option>
            <%
            console.log(categories.length);
             for(i=0; i<categories.length; i++){ 
                  console.log(categories); %>
              <option value="<%= categories[i].CategoryName%>"><%= categories[i].CategoryName%></option>
            <% } %>
          </select></p> 
          
                
                <p class="contact"><label for="ProductSalePrice">Product Sale Price</label></p> 
          <input id="ProductSalePrice" name="ProductSalePrice" placeholder="" tabindex="2" type="text"> 
           
                <p class="contact"><label for="ProductListPrice">ProductListPrice</label></p> 
          <input type="text" id="ProductListPrice" name="ProductListPrice" > 

                <p class="contact"><label for="ProductQuantityInStock">Product Quantity In Stock</label></p> 
