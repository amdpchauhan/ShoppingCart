  <script type="text/javascript">


    function validateform()
    {
      var flag=0;

      if($('#ProductName').val()=="")
      {
        $('#ProductName').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductName').css('border', '2px solid green');  
      }

      if($('#ProductCategory').val()=="")
      {
        $('#ProductCategory').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductCategory').css('border', '2px solid green');
      }

      if($('#ProductSalePrice').val()=="")
      {
        $('#ProductSalePrice').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductSalePrice').css('border', '2px solid green');  
      }

      if($('#ProductListPrice').val()=="")
      {
        $('#ProductListPrice').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductListPrice').css('border', '2px solid green');
      }

      if($('#ProductQuantityInStock').val()=="")
      {
        $('#ProductQuantityInStock').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductQuantityInStock').css('border', '2px solid green');
      }

      if($('#ProductLowStockLevel').val()=="")
      {
        $('#ProductLowStockLevel').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductLowStockLevel').css('border', '2px solid green');
      }

      if($('#ProductImageLink').val()=="")
      {
        $('#ProductImageLink').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductImageLink').css('border', '2px solid green');
      }

      if($('#ProductImageAltText').val()=="")
      {
        $('#ProductImageAltText').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductImageAltText').css('border', '2px solid green');
      }

      if($('#ProductShortDescription').val()=="")
      {
        $('#ProductShortDescription').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductShortDescription').css('border', '2px solid green');
      }

      if($('#ProductDetailedDescription').val()=="")
      {
        $('#ProductDetailedDescription').css('border', '2px solid red');
        flag = 1;
      }
      else
      {
        $('#ProductDetailedDescription').css('border', '2px solid green');
      }

      if(flag==1)
      {
        return false;
      }
    }
  </script>

    <style type="text/css"> 
        .form{
