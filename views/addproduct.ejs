          <input type="radio" name="ProductAvailable" value="Offline" />Offline 

          <p class="contact"><label for="ProductImage">Product Image</label></p> 
          <input type="file" name="ProductImage" value="upload Image"> 

          <p class="contact"><label for="ProductImageLink">Product Image Link</label></p> 
          <input type="text" id="ProductImageLink" name="ProductImageLink" />

